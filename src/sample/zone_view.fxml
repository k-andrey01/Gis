<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="405.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.ZoneController">
      <Label layoutX="14.0" layoutY="27.0" text="Время, прошедшее после разрушения объекта, ч" />
      <Label layoutX="14.0" layoutY="56.0" text="Температура воздуха, ℃" />
      <TextField fx:id="timeTextField" layoutX="300.0" layoutY="23.0" />
      <TextField fx:id="tempTextField" layoutX="300.0" layoutY="61.0" />
      <Label layoutX="128.0" layoutY="126.0" text="Укажите запасы СДЯВ на объекте и соответствующую массу, т" />
      <CheckBox fx:id="chlorCheckBox" layoutX="53.0" layoutY="182.0" mnemonicParsing="false" text="Хлор" />
      <CheckBox fx:id="ammiakCheckBox" layoutX="53.0" layoutY="231.0" mnemonicParsing="false" text="Аммиак" />
      <CheckBox fx:id="nitrilCheckBox" layoutX="53.0" layoutY="285.0" mnemonicParsing="false" text="Нитрил акриловой кислоты" />
      <TextField fx:id="chlorTextField" layoutX="256.0" layoutY="178.0" />
      <TextField fx:id="ammiakTextField" layoutX="256.0" layoutY="227.0" />
      <TextField fx:id="nitrilTextField" layoutX="256.0" layoutY="281.0" />
      <Button onAction="#onClickOkBtn" layoutX="207.0" layoutY="348.0" mnemonicParsing="false" prefWidth="80" text="ОК" />
      <Button onAction="#onClickCancelBtn" layoutX="313.0" layoutY="348.0" mnemonicParsing="false" prefWidth="80" text="Отмена" />
</AnchorPane>
